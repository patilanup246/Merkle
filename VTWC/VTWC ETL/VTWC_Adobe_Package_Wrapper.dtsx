                }                
            }
            Dts.Variables["User::vExtractFilesCount"].Value = iExtractCount;

            DateTime dt = DateTime.Now;
            dt = dt.AddDays(-1);
            string replaceDate = dt.ToString("yyyyMMdd");

            //check duplicates
            int chartoskip = sAdobeFolder.Length + 3;
            System.IO.DirectoryInfo dir = new System.IO.DirectoryInfo(sAdobeFolder);
            IEnumerable<System.IO.FileInfo> fileList = dir.GetFiles("*.*", System.IO.SearchOption.AllDirectories);
            var queryDupFiles = from file in fileList
                                group file.FullName.Substring(chartoskip) by
                                new FileDetails { Name = file.Name, CreationTime = file.CreationTime, Length = file.Length } into fileGroup
                                where fileGroup.Count() > 1
                                select fileGroup;
            var list = queryDupFiles.ToList();
            int i = queryDupFiles.Count();
            if (i > 0)
            {
                Dts.Variables["User::vIsDuplicateFileExists"].Value = 1;
            }
            else
            {
                Dts.Variables["User::vIsDuplicateFileExists"].Value = 0;
            }
            Dts.TaskResult = (int)ScriptResults.Success;
		}

